    /* --------------------------o2o筛选----------------- */
    /* 筛选选择 */
    .screen-box {
        width: 100%;
        height: 100vh;
        position: fixed;
        top: -100vh;
        left: 0;
        z-index: 1006;
        transition: ease all .6s;
    }
    /* show-screen */
    .show-screen {
        top: 0;
        transition: ease all .4s;
    }
    /* 内部遮罩速度慢（点击关闭用，因为外层遮罩层级底） */
    .close-screen-bg {
        width: 100%;
        height: 100%;
        background:rgba(0,0,0,0);
        position: absolute;
        left: 0;
        top: 0;
        z-index: 1007;
    }
    /* 外层遮罩速度快 */
    .screen-bg {
        width: 100%;
        height: 100vh;
        background:rgba(0,0,0,0.55);
        position: fixed;
        top: -100vh;
        left: 0;
        z-index: 1005;
        transition: ease all 0s;
    }
    .show-screen-bg {
        top: 0;
        transition: ease all 0s;
    }
    /* 筛选 内容部分样式 */
    .screen-box .content-box {
        width: 100%;
        height: 963rpx;
        position: absolute;
        top: 0;
        left: 0;
        background: #fff;
        /* padding: 24rpx 24rpx 100rpx 24rpx; */
        box-sizing: border-box;
        z-index: 1008;
        overflow: visible;
    }
    .screen-box .content-box .screen-header {
        width: 100%;
        height: 88rpx;
        line-height: 88rpx;
        font-size: 36rpx;
        color: #333333;
        text-align: center;
        position: relative;
        padding: 0 70rpx;
        box-sizing: border-box;
    }
    .screen-header .screen-close {
        position: absolute;
        left: 30rpx;
        top: 50%;
        margin-top: -17rpx;
        width: 34rpx;
        height: 34rpx;
    }
    /* 筛选项 */
    .content-box .check-box {
        height: 785rpx;
        width: 100%;
        padding: 0 20rpx;
        box-sizing: border-box;
    }
    .check-box .check-title {
        font-size: 28rpx;
        color: #333333;
        line-height: 40rpx;
        width: 100%;
        float: left;
        padding: 0 10rpx;
        box-sizing: border-box;
    }
    .check-box .check-type {
        width: 100%;
        padding: 10rpx 0 20rpx 0;
        box-sizing: border-box;
    }
    .check-type .type-item {
        width: 156rpx;
        height: 66rpx;
