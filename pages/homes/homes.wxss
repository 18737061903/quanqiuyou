/* pages/homes/homes.wxss */
page {
  background-color: #fff;
  width: 100%;
}
.clearfix {
    zoom: 1;
}
.clearfix:after {
    display:block;
    content:'';
    clear:both;
}
.line {
    height: .5rpx;
    width: 100%;
    background: #DBDBDB;
}
/* 头部导航 */
.header {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    position: fixed;
    top: 0;
    left: 0;
    padding: 0 102rpx;
    box-sizing: border-box;
    z-index: 1005;
}
/* 菜单 */
.header .menu{
    width: 105rpx;
    height: 88rpx;
    line-height: 88rpx;
    position: fixed;
    left: 0;
    top: 0;
    background: #fff;
    z-index: 9999
}
.header .menu .menu-img{
    width: 42rpx;
    height: 34rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -21rpx;
    margin-top: -17rpx;
}
/* 扫码 */
.header .yard {
    width: 102rpx;
    height: 88rpx;
    line-height: 88rpx;
    position: fixed;
    right: 0;
    top: 0;
    background: #fff;
    z-index: 99999
}
.header .yard .yard-img{
    width: 42rpx;
    height: 42rpx;
    position: absolute;
    right: 50%;
    top: 50%;
    margin-right: -21rpx;
    margin-top: -21rpx;
}
/* 导航按钮 */
.header .tab-btn {
    width: 600rpx;
    height: 88rpx;
    line-height: 88rpx;
    color: #333333;
    text-align: center;
    position: fixed;
    top: 0rpx;
    left: 99rpx;
    z-index: 999;
    background: #fff;
}
.header .tab-btn .tab-item{
    height: 50rpx;
    line-height: 45rpx;
    color: #333333;
    display: inline-block;
    margin-right: 45rpx;
    font-size: 32rpx;
   text-align: center;
}
.linenav{
    width:32rpx;
    height:6rpx;
    background:rgba(232,83,82,1);
    border-radius:25%;
    margin: 8rpx 18rpx 3rpx 18rpx;
    box-sizing: border-box;
    /* overflow: hidden; */
}
.header .tab-btn .tab-item-active {
    height: 50rpx;
    line-height: 45rpx;
    color: #E85352;
    display: inline-block;
    margin-right: 45rpx;
    font-size: 36rpx;
    transition: all ease .3s;
    font-weight: 500;
}
.nav {
  padding-top: 0rpx; 
  margin: 0rpx auto;
  width: 50%;
  height: 50rpx;
  display: flex;
  flex-direction: row;
}
.navsaoma{
    position: absolute;
    left: 0;
    /* padding-top: 10rpx; 
    margin-left: 20rpx; */
}
.navsaoma image{
  width:42rpx;
  height:38rpx;
}
.navsaoma1{
  position: absolute;
  float: right;
  right:0rpx; 
  padding-top: 10rpx; 
  margin-right: 20rpx;
}
.navsaoma1 image{
  width:42rpx;
  height:38rpx;
}
.default {
  line-height: 50rpx;
  text-align: center;
  flex: 1;
  color: #000;
  font-size: 32rpx;
}

.red {
  line-height: 50rpx;
  padding-bottom: 10rpx;
  text-align: center;
  color: #E85352;
  flex: 1;
  border-bottom: 2px solid  #E85352 ;
  font-size: 32rpx;
  font-weight: bold;
}
.show {
    display: block;
    padding: 0rpx 30rpx
}
.hidden {
  display: none;
}
