/* pages/orderDetail/orderDetail.wxss */
.page{
  width: 100%;
  height: 100vh;
  background: #F4F4F4;
  /* padding-bottom: 100rpx;  */
}
.addressdetail{
  width: 100%;
  padding: 30rpx 30rpx;
  background: #fff;
  box-sizing: border-box;
  position: relative
}
.addresstop text:first-child{
  font-size:32rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(51,51,51,1);
  display:inline-block;
  line-height: 45rpx
}
.addressname{
    line-height: 45rpx;
    display:inline-block;
    max-width: 160rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.addressphone{
    line-height: 45rpx;
    display:inline-block;
    max-width: 260rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.addressup{
  padding: 10rpx 70rpx 10rpx 0rpx;
}
.addressup text{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:37rpx;
}
.notaddress{
   width: 100%;
   box-sizing: border-box;
   padding: 10rpx 219rpx 10rpx 219rpx;
   clear: both
}
.nottop{
    clear: both
}
.nottop image{
    width: 35rpx;
    height: 43rpx;
    float: left;
    padding-left: 8rpx;
}
.nottop text{
    font-size:32rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:45rpx;
    float: right
}
.notup{
    clear: both
}
.notup text{
    font-size:28rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(102,102,102,1);
    line-height:40rpx;
}
.sm_right{
  position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -23rpx
}
.sm_right image{
  width: 23rpx;
  height: 23rpx;
}
.sm_bottom{
  width: 100%;
  height: 8rpx;
  position: absolute;
  bottom: 23rpx;
  left: 0rpx;
}
.sm_bottom image{
   width: 100%;
   height: 100%;
}
.store-goos{
  width: 100%;
  background: #fff;
  margin-top: 30rpx;
}
.goos-detail{
  width: 100%;
  padding: 30rpx 30rpx 0rpx 30rpx;
  box-sizing: border-box;
}
.goos-picker{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  clear: both;
  display: flex;
  justify-content: left;
}
.picker{
  width: 38rpx;
  height: 38rpx;
  display: inline-block;
  margin-right: 14rpx;
  border-radius: 6rpx;
}
.picker image{
  width: 100%;
  height: 100%;
  display: inline-block;
}
.goos-picker text{
  display: inline-block;
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:40rpx;
}
.text-img{
  width: 100%;
  padding: 24rpx 0rpx 30rpx 0rpx;
  display: flex;
  justify-content: left;
  box-sizing: border-box;
  overflow: hidden;
}
.imgs{
  width:180rpx;
  height:180rpx;
  border-radius: 6rpx;
}
.imgs .image{
  width:180rpx;
  height:180rpx;
  border-radius: 6rpx;
}
.text{
 padding-left:30rpx;
 padding-right:30rpx;  
 box-sizing: border-box;
}
.textdetail{
  width:480rpx;
  height:80rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:40rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
 -webkit-box-orient: vertical;
}
.texttype{
  margin-top: 6rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:37rpx;
  display: block
}
.price{
  margin-top: 6rpx;
  font-size:38rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(232,83,82,1);
  line-height:53rpx;
  display: block;
  clear: both
}
.price text{
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(232,83,82,1);
  line-height:33rpx;
}
.number{
  font-size:24rpx!important;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1) !important;
  line-height:52rpx!important;
  float: right;
  padding-right: 0rpx;
}
.goos-postage{
  width: 100%;
  padding: 27rpx 30rpx;
  position: relative;
  box-sizing: border-box;
}
.goos-postage text:first-child{
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:42rpx;
}
.gs-right{
  position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -20rpx;
}
.gs-right text{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:40rpx;
  padding-right: 0rpx;
}
.gs-right_img{
  width: 23rpx;
  height: 23rpx;
}
.favourable{
  width: 100%;
  padding: 0rpx 0rpx;
  background: #fff;
  box-sizing: border-box;
  margin-top: 20rpx;
  margin-bottom: 100rpx;
}
.favourable-top{
  width: 100%;
  position: relative;
  padding: 27rpx 30rpx;
  border-bottom: 1rpx #F4F4F4 solid;
    box-sizing: border-box;
}
.favourable-top text{
    font-size:30rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(102,102,102,1);
    line-height:42rpx;
}
.favourable-up{
  width: 100%;
  position: relative;
  padding: 27rpx 30rpx;
  border-bottom: 1rpx #F4F4F4 solid;
    box-sizing: border-box;
}
.favourable-up text{
    font-size:30rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(102,102,102,1);
    line-height:42rpx;
}
.gs-rights{
  position: absolute;
  right: 0rpx;
  top: 50%;
  margin-top: -20rpx;
}
.gs-rights text{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:#E85352;
  line-height:40rpx;
  padding-right: 30rpx;
}
.gs-right_imgs{
  width: 23rpx;
  height: 23rpx;
}
.submit{
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  height: 100rpx;
  background: #fff;
  line-height: 100rpx;
  clear: both;

}
.submitorder{
  width: 250rpx;
  height: 100rpx;
  background: #E85352;
  font-size:36rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:100rpx;
  float: right;
  text-align: center;
}
.submit-price{
  font-size:52rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(232,83,82,1);
  line-height:73rpx;
  padding-left: 30rpx;
}
.submit-price text:first-child{
  font-size:36rpx;
}

