<!--pages/successPay/successPay.wxml-->
<view class='page'>
   <view class='mian'>
       <view class='center'>
           <view class='img'><image lazy-load src='../../image/createOrder/success.png'></image></view>
            <text class='pay'>支付成功</text>
            <text class='paymoney'><text class='paymoney_l'>￥</text>{{payPrice}}</text>
       </view>
       <view class='homedetail'>
            <text bindtap='lookOrderDetail'>查看订单</text><text bindtap='goHomes'>去购物</text>
       </view>
   </view>
   <view class='sale'>
        <text>恭喜领取积分成功</text>
    </view>
    <view class='main-up'>
      <view class='main-n'>
        <view class='main-y'>
           <view class='main-f'>
              <text>积分</text>
              <text>500个</text>
           </view>
        </view>
      </view>
    </view>
</view>
